<view class="tabs">
  <view class="tabs_title">
    <view class="title_item" wx:for="{{tabs}}" wx:key="id" class="title_item  {{item.isActive?'active':''}} " data-index="{{index}}" bindtap="handleItemTap">
      {{item.value}}
    </view>
  </view>
  <view class="tabs_content">
    <slot></slot>
  </view>
</view>