@charset "UTF-8";
/**index.wxss**/
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  padding: 20rpx;
  flex: 1;
}

.index_cate navigator image {
  width: 100%;
}

.floor_group .floor_title {
  padding: 20rpx 0 10rpx;
}

.floor_group .floor_title image {
  width: 100%;
}

.floor_group .floor_list {
  overflow: hidden;
}

.floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  /* box-sizing: border-box; */
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}

.floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}

.floor_group .floor_list navigator:nth-last-child(-n + 4) {
  /* 原图的宽高 232 *386 */
  height: 27.72711207vw;
  /* padding-left: 10rpx; */
  border-left: 10rpx solid #fff;
}

.floor_group .floor_list navigator:nth-child(2),
.floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}

